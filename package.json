{
  "name": "FlatWice-FE",
  "version": "0.0.1",
  "private": true,
  "description": "FlatWice - app for rent",
  "scripts": {
    "dev": "node src/server.js",
    "start": "npm run build && npm run start:server",
    "start:prod": "cross-env PORT=80 npm run build && npm run start:forever",
    "start:forever": "cross-env NODE_ENV=production PORT=80 forever start src/server.js",
    "start:server": "cross-env NODE_ENV=production node src/server.js",
    "build": "next build src",
    "lint": "eslint -c .eslintrc .",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest",
    "test:e2e": "webdriver-manager update && protractor",
    "analyze": "cross-env ANALYZE=1 npm run build",
    "locales:po": "sh ./locales/extract_lang.sh",
    "locales:json": "sh ./locales/po2json.sh"
  },
  "jest": {
    "setupFiles": [
      "./__test__/beforeTest.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.next/",
      "/src/.next/"
    ],
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/next.config.js",
      "!src/.next/**",
      "!src/server.js",
      "!src/routes.js",
      "!src/mocks/flatListGenerator.js"
    ],
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "moduleNameMapper": {
      ".*[.](gif|ttf|eot|svg|png|jpg)$": "<rootDir>/__test__/utils/fileMock.js"
    }
  },
  "dependencies": {
    "autoprefixer": "7.1.4",
    "axios": "0.16.2",
    "babel-cli": "6.26.0",
    "babel-plugin-inline-react-svg": "0.4.0",
    "bytesize-icons": "1.2.0",
    "compression": "1.7.1",
    "cross-env": "5.0.5",
    "enzyme-adapter-react-16": "^1.0.1",
    "express": "4.15.5",
    "forever": "0.15.3",
    "jed": "1.1.1",
    "next": "beta",
    "next-redux-wrapper": "1.3.4",
    "next-routes": "1.0.40",
    "po2json": "0.4.5",
    "progress-bar-webpack-plugin": "1.10.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-ga": "^2.3.5",
    "react-redux": "5.0.6",
    "redux": "3.7.2",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.2.0",
    "styled-jsx": "^1.0.11",
    "uuid": "3.1.0"
  },
  "devDependencies": {
    "axios-mock-adapter": "1.9.0",
    "babel-eslint": "8.0.1",
    "babel-jest": "21.2.0",
    "chromedriver": "2.32.3",
    "enzyme": "^3.1.0",
    "eslint": "4.7.2",
    "eslint-config-hardcore-react": "0.3.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "jest": "21.2.1",
    "node-fetch": "1.7.3",
    "protractor": "5.1.2",
    "query-string": "^5.0.0",
    "react-dev-utils": "4.1.0",
    "react-test-renderer": "16.0.0",
    "redux-mock-store": "1.3.0",
    "selenium-standalone": "6.9.0",
    "sinon": "4.0.0",
    "webpack-bundle-analyzer": "2.9.0"
  },
  "main": "pages/index.js",
  "keywords": [
    "react",
    "webpack2",
    "redux",
    "jest",
    "nextjs",
    "windows",
    "i18n"
  ],
  "author": {
    "name": "Andrii Tiertyshnyi",
    "email": "andrii.tiertyshnyi@gmail.com"
  },
  "contributors": [
    {
      "name": "Andrii Tiertyshnyi(Faradey27)",
      "url": "https://github.com/Faradey27"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Faradey27/FlatWice-FE"
  },
  "bugs": {
    "url": "https://github.com/Faradey27/FlatWice-FE/issues"
  },
  "engines": {
    "node": ">= 8.4.0"
  }
}
